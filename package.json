    "webpack-dev-server": "=1.14.1",
    "webpack-merge": "=0.14.1",
    "webpack-validator": "=2.2.6"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-trailing-function-commas"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "build": {
    "appId": "io.lisk.nano",
    "productName": "Lisk Nano",
    "win": {
      "target": "nsis"
    }
  }
}
